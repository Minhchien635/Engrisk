<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" spacing="16.0" stylesheets="@../css/main.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.engrisk.controllers.ExamTableController">
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
    </padding>
    <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="16.0" VBox.vgrow="NEVER">
        <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onCreateClick" text="Thêm" textOverrun="CLIP" />
        <Button mnemonicParsing="false" onAction="#onEditClick" text="Sửa">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#onDeleteClick" text="Xóa">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Button>
    </HBox>
    <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="nameColumn" editable="false" minWidth="100.0" prefWidth="-1.0" text="Tên khóa thi" />
            <TableColumn fx:id="typeColumn" editable="false" minWidth="100.0" prefWidth="-1.0" text="Trình độ" />
            <TableColumn fx:id="priceColumn" editable="false" minWidth="100.0" prefWidth="-1.0" text="Lệ phí" />
            <TableColumn fx:id="dateColumn" editable="false" minWidth="100.0" prefWidth="-1.0" text="Ngày thi" />
        </columns>
    </TableView>
</VBox>
